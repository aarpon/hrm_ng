{
  "name": "hrm/hrm",
  "description": "The Huygens Remote Manager is an easy to use interface to the Huygens Software by Scientific Volume Imaging B.V. that allows for multi-user, large-scale deconvolution and analysis",
  "license": "GPL-2.0+",
  "keywords": [
    "deconvolution",
    "batch",
    "multi-user"
  ],
  "authors": [
    {
      "name": "Aaron Ponti",
      "email": "aarpon@gmail.com"
    }
  ],
  "require": {
    "ext-ldap": "*",
    "php": ">=7.2.0",
    "propel/propel": ">=2.0.0-beta1",
    "adldap/adldap": ">=5.0.0.x-dev",
    "monolog/monolog": ">=2.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.25",
    "phpdocumentor/phpdocumentor": ">=v3.0.0"
  },
  "autoload": {
    "psr-4": {
      "hrm\\": "src/hrm/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
